CREATE TABLE `clientes` (
`id` int( 10 ) unsigned AUTO_INCREMENT ,
`Grupo_Controlador` varchar( 100 ) NOT NULL ,
`Nome_Empresa` varchar( 100 ) NOT NULL ,
`Endereco_Empresa` varchar( 100 ) NOT NULL ,
`Cidade` varchar( 100 ) NOT NULL ,
`Estado` varchar( 100 ) NOT NULL ,
`CNPJ` varchar( 100 ) NOT NULL ,
`Incricao_Estadual` varchar( 100 ) NOT NULL ,
`Web_Site` varchar( 100 ) NOT NULL ,
`Numero_Funcionario` varchar( 100 ) NOT NULL ,
`Ramo_Atividade` varchar( 100 ) NOT NULL ,
`Historico_Cliente` varchar( 100 ) NOT NULL ,
`Nome_Contato` varchar( 100 ) NOT NULL ,
`Cargo_Contato` varchar( 100 ) NOT NULL ,
`Email_Contato` varchar( 100 ) NOT NULL ,
`Telefone_Contato` varchar( 100 ) NOT NULL ,
PRIMARY KEY ( `id` )
)

